        ans1=fibb(n-1,dp)
        dp[n-1]=ans1
    else:
        ans=dp[n-1]
    if dp[n-2]==-1:
        ans1=fibb(n-2,dp)
        dp[n-2]=ans1
    else:
        ans=dp[n-2]
    
